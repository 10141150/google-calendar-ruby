#div1.estilo-div
  %h2
    = t('elimevent')
.intro.grid_3.push_9{:align => "right"}
  = form_tag({:controller => "sistema", :action => "event_delete"}, :method => "get") do
    = label_tag "lbl_key", t('dev')
    = text_field_tag "txt_key", "", :size => 50, :maxlenght => 50, :placeholder => "key", :required => "required"
    = label_tag "lbl_path_key", t('pathp12')
    = text_field_tag "txt_path_key", "", :size => 50, :maxlenght => 50, :placeholder => "path_key", :required => "required"
    = label_tag "lbl_cuenta_developper", t('accountdev')
    = text_field_tag "txt_cuenta_developper", "", :size => 50, :maxlenght => 50, :placeholder => "cuenta_developper", :required => "required"
    = label_tag "lbl_calendar_id", t('calid')
    = text_field_tag "txt_calendar_id", "", :size => 50, :maxlenght => 50, :placeholder => "calendar_id", :required => "required"
    = label_tag "lbl_event_id", t('eventid')
    = text_field_tag "txt_event_id", "", :size => 50, :maxlenght => 50, :placeholder => "event_id", :required => "required"
    %br/
    = submit_tag t('send')
